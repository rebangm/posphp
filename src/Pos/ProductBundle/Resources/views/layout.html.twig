{% extends "::layout.html.twig" %}

{% block stylesheets %}

    {{parent() }}
    {%stylesheets output='css/compiled/warehouse.css' filter='cssrewrite'
                "bundles/posproduct/css/*"
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

{% endblock %}
{% block javascripts %}
    {{parent() }}
    {%stylesheets output='js/compiled/warehouse.js'
                "bundles/posproduct/js/*"
    %}
        <script type="text/javascript" src="{{ asset_url }}" /></script>
    {% endstylesheets %}
{% endblock %}



{% block leftmenu %}
    {#<ul class="nav nav-pills nav-stacked">#}
    <ul class="">
            <li><a href="{{ path('pos_product_manage') }}">Produits</a></li>
            <li><a href="{{ path('pos_product_manage_add') }}">Ajouter un produit</a></li>
            <li><a href="{{ path('pos_supplier_manage_list') }}">Fournisseurs</a></li>
            <li><a href="{{ path('pos_supplier_manage_list') }}">Ajouter un fournisseur</a></li>
    </ul>
{% endblock %}

{% if form is defined %}
    {% form_theme form 'PosHomeBundle:Form:form_theme_layout.html.twig' %}
{% endif %}

