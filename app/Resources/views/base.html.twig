<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Pos{% endblock %}</title>
 
    {% block stylesheets %}
        <link href="{{ asset("css/bootstrap-responsive.min.css") }}" rel="stylesheet" media="screen" />
        <link href="{{ asset("css/bootstrap.min.css") }}" rel="stylesheet" media="screen" />
        {% stylesheets output='css/compiled/common.css' filter='cssrewrite,yui_css'               
                "css/main.css"
        %}
            
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets%}
    {% endblock %}
    
    {% block icon %}
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('apple-touch-icon-144-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('apple-touch-icon-114-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('apple-touch-icon-72-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" href="{{ asset('apple-touch-icon-57-precomposed.png') }}">
    <link rel="shortcut icon" href="{{ asset('favicon.png') }}">
    {% endblock %}
     <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="../assets/js/html5shiv.js"></script>
    <![endif]-->

    <!-- Fav and touch icons -->
    
  </head>
 
  <body>
      
  {% block body %}
  {% endblock %}
  {% block footer %}
  {% endblock %}
  {% block javascripts %}
     {% javascripts output='js/compiled/common.js' filter='yui_js' 
            'js/main.js'
     %}

        <script src="{{ asset_url }}"></script>
     {% endjavascripts %}
    {# Ajoutez ces lignes JavaScript si vous comptez vous servir des fonctionnalit√©s du bootstrap Twitter #}
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script type="text/javascript" src="{{ asset('js/bootstrap.min.js') }}"></script>
  {% endblock %}
 
  </body>
</html>